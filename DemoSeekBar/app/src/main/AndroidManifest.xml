<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.demo_seekbar.pulkit">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.SimpleSeekbarActivity" />
        <activity android:name=".activities.SimpleRangeSeekbarActivity" />
        <activity android:name=".activities.PinRangeSeekbarActivity" />
        <activity android:name=".activities.AnimatedPinSeekbarActivity" />
        <activity android:name=".activities.CustomSeekbarActivity" />
        <activity android:name=".activities.BubbleRangeSeekbarActivity"/>



    </application>

</manifest>